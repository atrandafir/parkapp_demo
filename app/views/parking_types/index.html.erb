<h1>Listing parking_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Color</th>
    <th>Background</th>
    <th>Position</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parking_types.each do |parking_type| %>
  <tr>
    <td><%= parking_type.name %></td>
    <td><%= parking_type.color %></td>
    <td><%= parking_type.background %></td>
    <td><%= parking_type.position %></td>
    <td><%= link_to 'Show', parking_type %></td>
    <td><%= link_to 'Edit', edit_parking_type_path(parking_type) %></td>
    <td><%= link_to 'Destroy', parking_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

